{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@blog/*": ["./notion-blog/*"],
      "@portfolio/components/*": ["./components/about-portfolio/*"],
      "@portfolio/components": ["./components/about-portfolio/index"],
      "@portfolio/animation/*": ["./components/about-portfolio/animation/*"],
      "@portfolio/animation": ["./components/about-portfolio/animation/index"],
      "@portfolio/constants": ["./constants/about-portfolio"],
      "@portfolio/motion": ["./motion/about-portfolio"],
      "@portfolio/types": ["./types/about-portfolio"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
